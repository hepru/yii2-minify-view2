{
  "name": "hepru/yii2-minify-view2",
  "description": "Yii2 View component with auto minification css & js in runtime",
  "keywords": [
    "yii2",
    "minify",
    "view",
    "js",
    "css"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "yiisoft/yii2": "^2.0",
    "mrclay/minify": "3.0.7",
    "pimple/pimple": "~3.2.0"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "~3.1.0",
    "phpunit/phpunit": "^6.0",
    "monolog/monolog": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "hepru\\yii\\minify\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
